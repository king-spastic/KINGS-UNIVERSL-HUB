local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KINGS UNIVERSIAL HUB", "DarkTheme")
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")


    MainSection:NewButton("Back/Front Flip", "click x to do a backflip and click z to do a frontflip", function()
        loadstring(game:HttpGet('https://pastebin.com/raw/7CaZE3Ke'))()
    end)
	
	MainSection:NewButton("Roblox Anti Afk Script", "Prevents you from getting kicked while afk", function()
        loadstring(game:HttpGet('https://pastebin.com/raw/7wDcPtLk'))()
    end)

    MainSection:NewToggle("Super-Human", "go fast and jump high", function(state)
        if state then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
        else
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        end
    end)
    
            MainSection:NewButton("INF JUMP", "Hold space to inf jump release to stop.", function()
        loadstring(game:HttpGet('https://pastebin.com/raw/7hx82nmE'))()
    end)
	
     MainSection:NewButton("Infinite Yield", "FE Admin Commands", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
    end)
    
        MainSection:NewButton("universal aimbot", "aimbot that works on most games", function()
        loadstring(game:HttpGet(('https://pastebin.com/raw/iMw9cpn8'),true))()
    end)


    --LOCAL PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")

    PlayerSection:NewSlider("Walkspeed", "SPEED!!", 500, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)

    PlayerSection:NewSlider("Jumppower", "JUMP HIGH!!", 350, 50, function(s)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)

    PlayerSection:NewButton("Reset WS/JP", "Resets to all defaults", function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end)
	
	--Other
    local Other = Window:NewTab("Other")
    local OtherSection = Other:NewSection("Other")

    OtherSection:NewButton("Chat Spoofer", "Lets you chat for other people", function()
        loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
    end)

    OtherSection:NewButton("Bypassed Fly", "bird mode", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Nicuse/RobloxScripts/main/BypassedFly.lua"))() 

        Fly(true)
    end)
	
		--Da Hood
	local DaHood = Window:NewTab("DaHood")
    local HoodSection = DaHood:NewSection("Aim")
	
	HoodSection:NewButton("Silent Aim", "Good semi legit aimlock for Da Hood.", function()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/ribrZ9UZ"))()
end)

	HoodSection:NewButton("AntiLock", "Da Hood AntiLock (click x to turn on)", function()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/DLrzE15p"))()
end)

	HoodSection:NewButton("Ass Cam Lock", "Goofy ahh camlock (not recommended ", function()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/XYyFxqG7"))()
end)

	HoodSection:NewButton("Aim Viewer", "Aim viwer to see if people are locking", function()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/3gsnUJbY"))()
end)

	HoodSection:NewButton("Blatant AimLock", "Q to toggle", function()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/UwTknCgF"))()
end)
